{
    "name": "myproject/sample",
    "type": "project",
    "description": "A sample project with various dependency syntax",
    "require": {
        "php": ">=7.4",
        "monolog/monolog": "2.3.5",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/console": "5.3.*",
        "laravel/framework": "8.65.0@beta",
        "vendor/package": "dev-feature-branch"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "replace": {
        "psr/log": "1.1.4"
    },
    "conflict": {
        "monolog/monolog": "<2.0"
    },
    "provide": {
        "psr/log-implementation": "1.0.0"
    },
    "suggest": {
        "ext-redis": "For using Redis cache"
    }
}
